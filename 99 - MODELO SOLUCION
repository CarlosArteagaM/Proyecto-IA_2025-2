      "cell_type": "code",
      "source": [
        "# ==========================================\n",
        "# 11. APLICAR EL MODELO A NUEVOS DATOS\n",
        "# ==========================================\n",
        "\n",
        "import pandas as pd\n",
        "\n",
        "# Columnas con correlación absoluta MENOR a 0.1 (según tu tabla anterior)\n",
        "columnas_baja_corr = [\n",
        "    \"REGION_ORINOQUIA\",\n",
        "    \"REGION_AMAZONIA\",\n",
        "    \"PERIODO_ACADEMICO\",\n",
        "    \"E_PRGM_EDUCACION\",\n",
        "    \"REGION_CARIBE\",\n",
        "    \"E_HORASSEMANATRABAJA\",\n",
        "    \"INDICADOR_4\",\n",
        "    \"E_PRGM_ADMINISTRACION_Y_FINANZAS\",\n",
        "    \"INDICADOR_2\",\n",
        "    \"E_PAGOMATRICULAPROPIO\"\n",
        "]\n",
        "\n",
        "print(\"\\n=== APLICAR MODELO A NUEVOS DATOS ===\")\n",
        "ruta_nuevo = input(\"Ingrese el nombre o ruta del nuevo CSV sin la columna objetivo: \")\n",
        "\n",
        "df_nuevo = pd.read_csv(ruta_nuevo)\n",
        "print(\"Dimensiones del nuevo dataset cargado:\", df_nuevo.shape)\n",
        "\n",
        "# ------------------------------------------\n",
        "# 1. Eliminar columnas de baja correlación\n",
        "# ------------------------------------------\n",
        "cols_a_eliminar = [c for c in columnas_baja_corr if c in df_nuevo.columns]\n",
        "\n",
        "print(\"\\nColumnas a eliminar por baja correlación:\")\n",
        "print(cols_a_eliminar)\n",
        "\n",
        "df_nuevo = df_nuevo.drop(columns=cols_a_eliminar)\n",
        "print(\"Dimensiones después de eliminar columnas:\", df_nuevo.shape)\n",
        "\n",
        "# ------------------------------------------\n",
        "# 2. Asegurar que las columnas coincidan con el modelo\n",
        "# ------------------------------------------\n",
        "# El modelo fue entrenado con X_train\n",
        "columnas_entrenamiento = X.columns\n",
        "\n",
        "# Agregar columnas faltantes con 0\n",
        "for col in columnas_entrenamiento:\n",
        "    if col not in df_nuevo.columns:\n",
        "        df_nuevo[col] = 0\n",
        "\n",
        "# Eliminar columnas extra no usadas en el entrenamiento\n",
        "df_nuevo = df_nuevo[columnas_entrenamiento]\n",
        "\n",
        "# ------------------------------------------\n",
        "# 3. Aplicar el mejor modelo (Random Forest)\n",
        "# ------------------------------------------\n",
        "print(\"\\nGenerando predicciones para los nuevos datos...\")\n",
        "\n",
        "pred_nuevo = mejor_modelo.predict(df_nuevo)\n",
        "\n",
        "# Decodificar (1–4 → bajo–alto)\n",
        "pred_nuevo_decod = [mapa_clases.get(v, \"desconocido\") for v in pred_nuevo]\n",
        "\n",
        "# Agregar al dataframe\n",
        "df_nuevo[\"RENDIMIENTO_GLOBAL\"] = pred_nuevo_decod\n",
        "\n",
        "print(\"Predicciones generadas:\", len(pred_nuevo_decod))\n",
        "print(\"Primeras 10 predicciones:\", pred_nuevo_decod[:10])\n",
        "\n",
        "# ------------------------------------------\n",
        "# 4. Guardar archivo con predicciones\n",
        "# ------------------------------------------\n",
        "salida = \"predicciones_nuevos_datos.csv\"\n",
        "df_nuevo.to_csv(salida, index=False)\n",
        "\n",
        "print(f\"\\nArchivo generado: {salida}\")\n",
        "print(\"Proceso terminado exitosamente.\")\n"
      ],
      "metadata": {
        "id": "JIkPcbcMvpKY",
