    {
      "cell_type": "code",
      "source": [
        "moda_internet = d8['F_TIENEINTERNET'].mode(dropna=True)[0]\n",
        "print(\"La moda de F_TIENEINTERNET es:\", moda_internet)\n",
        "\n",
        "d8['F_TIENEINTERNET'] = d8['F_TIENEINTERNET'].fillna(moda_internet)\n",
        "\n",
        "print(\"\\nValores únicos después de reemplazar NaN por la moda:\")\n",
